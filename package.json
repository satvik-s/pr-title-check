{
  "name": "pr-title-verify",
  "version": "1.2.1",
  "description": "PR title check GitHub action",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "lint": "eslint --fix src/**/*.ts",
    "lint:fix": "prettier --write '**/*.ts'",
    "package": "ncc build --license licenses.txt",
    "all": "npm run build && npm run lint && npm run package"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/satvik-s/pr-title-check"
  },
  "keywords": [
    "actions",
    "node",
    "setup",
    "PR",
    "title",
    "verify",
    "check"
  ],
  "author": "Satvik Shukla",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "1.10.0",
    "@actions/github": "5.1.1"
  },
  "devDependencies": {
    "@types/node": "20.2.5",
    "@typescript-eslint/eslint-plugin": "6.1.0",
    "@typescript-eslint/parser": "5.59.11",
    "@vercel/ncc": "0.36.1",
    "eslint": "8.42.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-eslint-comments": "3.2.0",
    "eslint-plugin-github": "4.8.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jest": "27.2.2",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-simple-import-sort": "10.0.0",
    "eslint-plugin-unused-imports": "2.0.0",
    "js-yaml": "4.1.0",
    "npm-check-updates": "16.10.12",
    "prettier": "2.8.8",
    "typescript": "5.0.4"
  }
}
