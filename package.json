{
  "name": "pr-title-verify",
  "version": "1.3.2",
  "description": "PR title check GitHub action",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "lint": "eslint --fix src/**/*.ts",
    "lint:fix": "prettier --write '**/*.ts'",
    "package": "ncc build --license licenses.txt",
    "all": "npm run build && npm run lint && npm run package"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/satvik-s/pr-title-check"
  },
  "keywords": [
    "actions",
    "node",
    "setup",
    "PR",
    "title",
    "verify",
    "check"
  ],
  "author": "Satvik Shukla",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "1.11.1",
    "@actions/github": "6.0.0"
  },
  "devDependencies": {
    "@types/node": "22.14.0",
    "@typescript-eslint/eslint-plugin": "8.29.1",
    "@typescript-eslint/parser": "8.29.1",
    "@vercel/ncc": "0.38.3",
    "eslint": "9.24.0",
    "eslint-config-prettier": "10.1.1",
    "eslint-plugin-eslint-comments": "3.2.0",
    "eslint-plugin-github": "6.0.0",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-jest": "28.11.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "5.2.6",
    "eslint-plugin-simple-import-sort": "12.1.1",
    "eslint-plugin-unused-imports": "4.1.4",
    "js-yaml": "4.1.0",
    "npm-check-updates": "17.1.16",
    "prettier": "3.5.3",
    "typescript": "5.8.3"
  }
}
